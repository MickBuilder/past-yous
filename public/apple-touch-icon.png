<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 100 100">
  <defs>
    <linearGradient id="bg" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#E6B800;stop-opacity:1" />
      <stop offset="50%" style="stop-color:#B8860B;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#8B6914;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="photo" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#4A90E2;stop-opacity:0.3" />
      <stop offset="50%" style="stop-color:#E6B800;stop-opacity:0.2" />
      <stop offset="100%" style="stop-color:#4A90E2;stop-opacity:0.3" />
    </linearGradient>
  </defs>
  
  <!-- Background circle with theme gradient -->
  <circle cx="50" cy="50" r="45" fill="url(#bg)" stroke="#2D1810" stroke-width="2"/>
  
  <!-- Polaroid card -->
  <rect x="25" y="20" width="50" height="60" rx="4" fill="white" stroke="#2D1810" stroke-width="1.5"/>
  
  <!-- Photo area with theme colors -->
  <rect x="30" y="25" width="40" height="35" rx="2" fill="url(#photo)"/>
  
  <!-- Decade text -->
  <text x="50" y="75" text-anchor="middle" font-family="monospace" font-size="12" font-weight="bold" fill="#2D1810">60s</text>
  
  <!-- Time travel arrow with primary theme color -->
  <path d="M15 50 L85 50 M80 45 L85 50 L80 55" stroke="#E6B800" stroke-width="3" fill="none" stroke-linecap="round" stroke-linejoin="round"/>
  
  <!-- Small accent dots -->
  <circle cx="20" cy="25" r="2" fill="#4A90E2" opacity="0.7"/>
  <circle cx="80" cy="75" r="2" fill="#E6B800" opacity="0.7"/>
</svg>
